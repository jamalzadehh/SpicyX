@model ChefCreateVM

<div class="col-md-6 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Create</h4>
            <p class="card-description">
                Chefs
            </p>
            <form class="forms-sample" enctype="multipart/form-data" method="post">
                <div class="form-group">
                    <label asp-for="FullName">FullName</label>
                    <input asp-for="FullName" class="form-control" placeholder="FullName">
                    <span class="text-danger" asp-validation-for="FullName" ></span>
                </div> 
                 <div class="form-group">
                    <label asp-for="Position">Position</label>
                    <input asp-for="Position" class="form-control" placeholder="Position">
                    <span class="text-danger" asp-validation-for="Position" ></span>
                </div> 
                 <div class="form-group">
                    <label asp-for="linkedin">Linkedin</label>
                    <input asp-for="linkedin" class="form-control" placeholder="Linkedin">
                    <span class="text-danger" asp-validation-for="linkedin" ></span>
                </div> 
                 <div class="form-group">
                    <label asp-for="Fcblink">Facebook</label>
                    <input asp-for="Fcblink" class="form-control" placeholder="Facebook">
                    <span class="text-danger" asp-validation-for="Fcblink" ></span>
                </div>
                <div class="form-group">
                    <label asp-for="twtlink">Twitter</label>
                    <input asp-for="twtlink" class="form-control" placeholder="Twitter">
                    <span class="text-danger" asp-validation-for="twtlink"></span>
                </div>
                 <div class="form-group">
                    <label asp-for="gglink">FullName</label>
                    <input asp-for="gglink" class="form-control" placeholder="FullName">
                    <span class="text-danger" asp-validation-for="gglink"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Image">Image</label>
                    <input asp-for="Image" class="form-control" placeholder="Image">
                    <span class="text-danger" asp-validation-for="Image"></span>
                </div>
                <div class="form-check form-check-flat form-check-primary">
                    <label class="form-check-label">
                        <input type="checkbox" class="form-check-input">
                        Remember me
                    </label>
                </div>
                <button type="submit" class="btn btn-primary me-2">Submit</button>
                <a type="button" asp-action="index" asp-controller="Chef" class="btn btn-light">Cancel</a>
            </form>
        </div>
    </div>
</div>
